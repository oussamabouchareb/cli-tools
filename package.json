{
  "name": "tdp-tools",
  "version": "1.0.2",
  "description": "a cli tool for the delta project",
  "homepage": "https://github.com/oussamabouchareb/cli-tools",
  "repository": "github:oussamabouchareb/cli-tools",
  "author": "oussama bouchareb <commensalism@protonmail.com> (https://github.com/oussamabouchareb)",
  "license": "MIT",
  "keywords": [
    "graphql",
    "cli",
    "graphql-tools",
    "typeorm"
  ],
  "main": "build/index.js",
  "source": "src/index.ts",
  "bin": {
    "tdp-tools": "build/index.js"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "LICENSE.md",
    "build"
  ],
  "scripts": {
    "start": "ts-node src/index.ts",
    "clean": "rimraf build",
    "build": "npm run clean && rollup -c",
    "publish:public": "npm publish --access=public",
    "prepublishOnly": "npm run format:check && npm run lint && npm run build",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'"
  },
  "dependencies": {
    "chalk": "*",
    "commander": "*",
    "inquirer": "*",
    "rollup-plugin-preserve-shebang": "^1.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@types/inquirer": "^8.1.3",
    "@types/node": "^8.10.66",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.59.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "chalk": "*",
    "commander": "*",
    "inquirer": "*"
  },
  "npmName": "tdp-tools",
  "npmFileMap": [
    {
      "basepath": "/build/",
      "files": [
        "*.js"
      ]
    }
  ]
}
